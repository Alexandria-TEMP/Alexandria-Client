{
  "extends": ["next/core-web-vitals", "prettier"],
  "overrides": [
    {
      "files": ["__tests__/**/*"],
      "env": {
        "jest": true
      }
    }
  ],
  "plugins": [
    "check-file"
  ],
  "rules": {
    "check-file/filename-naming-convention": ["error", { "app/**/*" : "KEBAB_CASE" }],
    "check-file/folder-naming-convention": ["error", { "app/*/" : "NEXT_JS_APP_ROUTER_CASE" }]
  }
}
